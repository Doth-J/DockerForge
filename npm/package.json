{
  "name": "dockerforged",
  "version": "1.0.1",
  "description": "CLI tool to generate dockerfile definitions and scaffold project templates",
  "author": "Doth-J",
  "main": "dist/index.mjs",
  "homepage": "https://github.com/Doth-J/DockerForge#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Doth-J/DockerForge.git"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "kleur": "^4.1.5",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/prompts": "^2.4.9",
    "rimraf": "^5.0.5",
    "terser": "^5.29.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "bin": {
    "dockerforge": "index.js"
  },
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.5.0"
  },
  "keywords": [
    "dockerforge",
    "dockerfiles",
    "docker-compose",
    "scaffold"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "dev": "ts-node index.ts",
    "build": "rimraf dist && tsc && terser dist/index.mjs -c -o dist/index.min.mjs",
    "compile": "rimraf bin && bun build src/index.mts --compile --minify --outfile ./bin/dockeforge",
    "start": "bun dist/index.mjs"
  }
}
